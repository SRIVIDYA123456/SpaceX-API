  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "blake3": "^2.1.4",
    "casbin": "^5.1.2",
    "cheerio": "^1.0.0-rc.3",
    "cron": "^1.8.2",
    "fuzzball": "^1.3.0",
    "got": "^11.5.2",
    "ioredis": "^4.17.3",
